

import java.util.Scanner;

public class Main {

	private static Scanner in;

	public static void main(String[] args) {
		in = new Scanner(System.in);
		int n;

		n = in.nextInt();
		int[] numbers = new int[n];
		
		int i = 0;
		while (i < n) {
			numbers[i]=in.nextInt();
			i++;
		}
		
		int min = numbers[0];
		int j = 0;
		while (j < n){
		  if (min > numbers[j]){
		    min = numbers[j];
		  }
		  j++;
		}
		System.out.printf("min = "+min);

		
	}
}
