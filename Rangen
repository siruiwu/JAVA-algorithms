
import java.util.Random;
import java.util.Scanner;

public class Main{
  
  private static Scanner in;

public static void main(String[] args){
	in = new Scanner(System.in);
	int n = in.nextInt();
	int m = in.nextInt();
	int k = in.nextInt();
	int a = 0;
	
	Random random = new Random(k);
	int i = 0;
	while (i<n){
	  a = random.nextInt(m);
	    i++;
	}
	
	System.out.print(a);
	
  }
}
